<beanio xmlns="http://www.beanio.org/2012/03">

	<!-- 'strict' enforces record order and record sizes -->
	<stream name="users" format="csv" ignoreUnidentifiedRecords="true" resourceBundle="validationMessages">
		<parser>
			<property name="delimiter" value="," />
		</parser>
		<record name="user"
			class="com.evolvus.sandstorm.csv.bean.UserBean" minOccurs="0" maxOccurs="unbounded"  >
			<field name="userId" required="true"  />
			<field name="userName" required="true" />
			<field name="designation"  />
			<field name="role" required="true" />
			<field name="phoneNumber"  />
			<field name="mobileNumber" />
			<field name="emailId" required="true" />
			<field name="fax" />
			<field name="country" required="true"/>
			<field name="state" required="true" />
			<field name="city" required="true" />
			<field name="masterTimeZone" />
			<field name="individualTransactionLimit" required="true" type="integer"/>
			<field name="dailyLimit" required="true" type="integer"/>
			<field name="masterCurrency" required="true" />
		</record>
	</stream>
</beanio>